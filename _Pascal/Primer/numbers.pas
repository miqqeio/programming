// 2020-02
program Numbers;

begin
    // expression
    writeln(12 + 2 * (40 / 4) + 10);
    // 4.2...

    // floating point conversion
    writeln(12.0 + (10 + 20));
    // 4.2...

    // integer division
    writeln(30 div 20);
    // 1

    // integer modulo (remainder)
    writeln(42 mod 10);
    // 2
end.